
<main class="usersdash">
    
<div class="wrapper">
  
  <div class="top_navbar">
    <div class="hamburger">
       <div class="one"></div>
       <div class="two"></div>
       <div class="three"></div>
    </div>
    <div class="top_menu">
      <div class="logo">
         DASHBOARD
      </div>
      <div class="logo" style="margin-left:auto;">
         SIGNOUT
      </div>
      <ul>
        <li><a href="/signout" data-method="delete">
          <i class="fas fa-user" ></i>
          </a></li>
      </ul>
    </div>
  </div>
  
  <div class="sidebar">
      <ul>
        <li><a href="/owner"style="text-decoration: none;" >
          <span class="icon"><i class="fas fa-book"></i></span>
          <span class="title">Menus</span>
          </a></li>
        <li><a href="#" class="active"style="text-decoration: none;">
          <span class="icon"><i class="	fas fa-child"></i></span>
          <span class="title">Users</span>
          </a></li>
        <li><a href="/clerkdash" style="text-decoration: none;">
          <span class="icon"><i class="fas fa-male"></i></span>
          <span class="title">Clerks</span>
          </a></li>
        <li><a href="/salesdash"style="text-decoration: none;" >
          <span class="icon"><i class="fab fa-btc"></i></span>
          <span class="title">Sales Report</span>
          </a></li>
        <li><a href="/orderdash"style="text-decoration: none;" >
          <span class="icon"><i class="fas fa-cart-plus"></i></span>
          <span class="title">Orders</span>
          </a></li>
    </ul>
  </div>
  <div class="main_container">
      <div class="border-scroll-clerk">
        <h1 style="color: #E62E5D;margin-left: auto;font-size:40px;" align="center">All Customers</h1>
        <div class="item-scrollbar-clerk">
          <ol>
               <%user=User.customer()%>
                <li class="menuitem">
                <%user.each do|user| %>
                  <label class="menuitem-container">
                  
                    <h3>
                      <%=user.first_name%>
                      </h3>
                      </label>
                   <%end%>
                </li> 
          </ol>
        </div>
      </div>
  </div>
</main>