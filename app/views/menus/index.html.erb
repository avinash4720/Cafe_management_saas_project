<div class="container-body">
        <div class="container-sign animated" id="container">
            <div class="form-container sign-up-container">
                <button id="close-btn-left"><i class="far fa-times-circle"></i></button>
                <div class="sign-logo">
                    <a class="navbar-brand" href=""><i class="fas fa-utensils nav-icon sign-icon"></i> <span class="logo-foody sign-foody">Foody</span> <span class="Blinders-logo">Blinders</span></a>
                </div>
                <form class="sign-form" action="#">
                    <h1 class="sign-h1">Create Account</h1>
                    <!-- <div class="social-container">
                        <a class="sign-a" href="#" class="social"><i class="fab fa-facebook-f"></i></a>
                        <a class="sign-a" href="#" class="social"><i class="fab fa-google-plus-g"></i></a>
                        <a class="sign-a" href="#" class="social"><i class="fab fa-linkedin-in"></i></a>
                    </div>
                    <span class="sign-span">or use your email for registration</span> -->
                    <input class="sign-input" type="text" placeholder="Name" />
                    <input class="sign-input" type="email" placeholder="Email" />
                    <input class="sign-input" type="password" placeholder="Password" />
                    <button class="sign-btn">Sign Up</button>
                </form>
            </div>
            <div class="form-container sign-in-container">
                <div class="sign-logo">
                    <a class="navbar-brand" href=""><i class="fas fa-utensils nav-icon sign-icon"></i> <span class="logo-foody sign-foody">Foody</span> <span class="Blinders-logo">Blinders</span></a>
                </div>
                <form class="sign-form" action="#">
                    <h1 class="sign-h1">Sign in</h1>
                    <!-- <div class="social-container">
                        <a class="sign-a" href="#" class="social"><i class="fab fa-facebook-f"></i></a>
                        <a class="sign-a" href="#" class="social"><i class="fab fa-google-plus-g"></i></a>
                        <a class="sign-a" href="#" class="social"><i class="fab fa-linkedin-in"></i></a>
                    </div>
                    <span class="sign-span">or use your account</span> -->
                    <input class="sign-input" type="email" placeholder="Email" />
                    <input class="sign-input" type="password" placeholder="Password" />
                    <a class="sign-a" href="#">Forgot your password?</a>
                    <button class="sign-btn">Sign In</button>
                </form>
            </div>
            <div class="overlay-container">
                <div class="overlay">
                    <div class="overlay-panel overlay-left">
                        <h1 class="sign-h1-tag">Order Now!</h1>
                        <p class="sign-para">To eat more delicious food, login with your personal info.</p>
                        <button class="ghost sign-btn" id="signIn">Sign In</button>
                    </div>
                    <div class="overlay-panel overlay-right">
                        <button id="close-btn-right"><i class="far fa-times-circle"></i></button>
                        <h1 class="sign-h1-tag">Hungry?</h1>
                        <p class="sign-para">Enter your personal details and start Ordering.</p>
                        <button class="ghost sign-btn" id="signUp">Sign Up</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

